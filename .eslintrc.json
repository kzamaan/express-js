{
    "env": {
        "commonjs": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base",
        "airbnb-base/legacy"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": 0,
        "no-tabs": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "operator-linebreak": [
            "error",
            "after",
            {
                "overrides": {
                    ":": "before",
                    "?": "before",
                    "||": "before"
                }
            }
        ]
    },
    "plugins": [
        "prettier"
    ]
}